<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.otoklixwiyatmoko">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.otoklixwiyatmoko.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Otoklikwiyatmoko">
        <activity
            android:name="com.otoklixwiyatmoko.activities.ActivityCreateBlog"
            android:parentActivityName="com.otoklixwiyatmoko.activities.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.otoklixwiyatmoko.activities.ActivityBlogDetail"
            android:parentActivityName="com.otoklixwiyatmoko.activities.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.otoklixwiyatmoko.activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>